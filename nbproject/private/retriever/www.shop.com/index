        StringMessageBundle.getMessage = function(key, replacements){
        if($.isArray(replacements)){
            var messageToReplace = StringMessageBundle.messages[key];
            for (var i = 0; i < replacements.size; i++){
                var placeholder = "{" + i + "}";
                messageToReplace = messageToReplace.replace(placeholder, replacements[i]);
            }
            return messageToReplace;
        }
        return StringMessageBundle.messages[key];
    };
